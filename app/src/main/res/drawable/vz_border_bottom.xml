<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <item>
        <shape android:shape="rectangle" >
            <stroke
                android:width="1dp"
                android:color="@color/ct_mf_light_grey_color" />
        </shape>
    </item>
    <item
        android:bottom="1dp"
        android:top="-2dp"
        android:right="-2dp"
        android:left="-2dp"
        >
        <shape android:shape="rectangle" >
            <solid android:color="@color/ct_mf_white_color" />
        </shape>
    </item>

</layer-list>